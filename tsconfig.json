{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "baseUrl": ".",
    "types": [
      "webpack-env",
      "node",
      "jest"
    ],
    "paths": {
      "#/*": [
        "./src/*"
      ],
      "#components/*": [
        "./src/components/*"
      ],
      "#composables/*": [
        "./src/composables/*"
      ],
      "#styles/*": [
        "./src/styles/*"
      ],
      "#assets/*": [
        "./src/assets/*"
      ],
      "#views/*": [
        "./src/views/*"
      ],
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue"
  ]
}
